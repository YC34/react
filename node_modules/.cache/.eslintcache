ent.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/App_branching.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/components/Heading07.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/components/Closing07.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/components/Navigation07.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/App_array_and_table.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/components/MemberList.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch03.react_component/components/Fruit.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch04.event_handing/App_event_test.js",[],[],[],"/Users/y_chan/Documents/GitHub/react/src/ch04.event_handing/ClickTestFile.js",["250"],[],"import {Component} from 'react';\n\nclass ClickTestComponent extends Component{\n  \n  ClickTest=(param) =>{\n    if(typeof param != 'string'){\n        param='Click Another';\n    }\n\n    console.log('param : '+ param);\n\n  }\n\n  render(){\n    return(\n        <>\n            <h2>클릭 이벤트</h2>\n            <button onClick={e => this.ClickTest('Click Button')}>버튼 클릭</button>\n            <div onClick={e => this.ClickTest('Click Div')}>div 클릭</div>\n            <a onClick={this.ClickTest}>앵커 클릭(파라미터 없음)</a>\n        </>\n      );\n  }\n\n}\n\n// 클래스 이름과 동일해야 합니다.\nexport default ClickTestComponent;","/Users/y_chan/Documents/GitHub/react/src/ch04.event_handing/ChangeTestFile.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch04.event_handing/MouseMoveTestFile.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch04.event_handing/SubmitTestFile.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch04.event_handing/KeyEventTestFile.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch05.styling_example/App_rounded_rectangle_01.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch05.styling_example/App_rounded_rectangle_02.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch05.styling_example/App_font_size_02.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch05.styling_example/App_inheritance_01.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch05.styling_example/App_button_01.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch05.styling_example/components/Button_01.js",[],[],"/Users/y_chan/Documents/GitHub/react/src/ch07.react_hooks/App_reducer_01.js",["251"],[],"import {useState, useReducer} from \"react\";\nimport './../App.css';\n\n/* 초기 값을 설정합니다. */\nconst initialState = {count: 0};\n\n/* 리듀서를 구현합니다. */\nfunction reducer(state, action) {\n\t/* 입력이 안되는 데이터는 undefined가 됩니다. */\n\tconsole.log('action.type='+action.type);\n\tconsole.log('action.min='+action.min);\n\tconsole.log('action.max='+action.max);\n\tconsole.log('action.step='+action.step);\t\t\n\t\n\tswitch (action.type){\t\t\n\t\tcase \"INCREMENT\":\n\t\t\treturn state.count < action.max ? {count: state.count + action.step} : state;\n\t\t\t\n\t\tcase \"DECREMENT\":\n\t\t\treturn state.count > action.min ? {count: state.count - action.step} : state;\n\t\t\t\n\t\tcase \"RESET\":\n\t\t\treturn initialState;\n\t\t\t\n\t\tcase \"RANDOM\":\n\t\t\treturn {count: Math.floor(Math.random() * (action.max - action.min)) + action.min,};\t\n\t\t\t\n\t\tdefault:\n\t\t\tthrow new Error(\"Unsupported action type:\", action.type);\n\t}\n}\t\n\nfunction App({step = 1, min = 0, max = 10}) {\t\t\n\t/* useReducer를 사용하여 state, dispatch 반환 받습니다. */\n\tconst [state, dispatch] = useReducer(reducer, initialState);\n\n\treturn (\n\t\t<>\n\t\t\t현재 값 : &nbsp;<span>{state.count}</span>\n\t\t\t<br/><br/>\n\t\t\t\n\t\t\t{/* 액션을 발생시킬 곳에 dispatch 함수를 적용합니다. */}\n\t\t\t<button onClick={() => dispatch({type: \"INCREMENT\", step, max})}>증가</button>\n\t\t\t&nbsp;\n\t\t\t<button onClick={() => dispatch({type: \"DECREMENT\", step, min})}>감소</button>\n\t\t\t&nbsp;\n\t\t\t<button onClick={() => dispatch({type: \"RANDOM\", min, max})}>무작위</button>\n\t\t\t&nbsp;\n      \t\t<button onClick={() => dispatch({type: \"RESET\"})}>초기화</button>\t\t\t\n\t\t</>\n\t);\n}\n \n\nexport default App;","/Users/y_chan/Documents/GitHub/react/src/ch08.Axios_rechart_chartJs/JsonPlaceholder.js",[],[],[],"/Users/y_chan/Documents/GitHub/react/src/ch08.Axios_rechart_chartJs/components/JsonPlaceApiTest.js",[],[],{"ruleId":"252","severity":1,"message":"253","line":7,"column":9,"nodeType":"254","messageId":"255","endLine":7,"endColumn":16},{"ruleId":"252","severity":1,"message":"256","line":41,"column":7,"nodeType":"254","messageId":"255","endLine":41,"endColumn":13},{"ruleId":"257","severity":1,"message":"258","line":28,"column":15,"nodeType":"259","messageId":"260","endLine":28,"endColumn":17},{"ruleId":"261","severity":1,"message":"262","line":8,"column":11,"nodeType":"263","endLine":8,"endColumn":79},{"ruleId":"261","severity":1,"message":"262","line":8,"column":11,"nodeType":"263","endLine":8,"endColumn":79},{"ruleId":"264","severity":1,"message":"265","line":20,"column":13,"nodeType":"263","endLine":20,"endColumn":41},{"ruleId":"252","severity":1,"message":"266","line":1,"column":9,"nodeType":"254","messageId":"255","endLine":1,"endColumn":17},"no-unused-vars","'jegob02' is assigned a value but never used.","Identifier","unusedVar","'func06' is assigned a value but never used.","array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/jsx-eslint/eslint-plugin-jsx-a11y/blob/HEAD/docs/rules/anchor-is-valid.md","'useState'